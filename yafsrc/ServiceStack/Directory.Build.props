<Project>

  <PropertyGroup>
	<Version>4.0.1</Version>
	<AssemblyVersion>4.0.1</AssemblyVersion>
	<FileVersion>4.0.1</FileVersion>
    <Authors>ServiceStack</Authors>
    <Company>ServiceStack, Inc.</Company>
    <Copyright>Â© 2008-2025 ServiceStack, Inc</Copyright>
    <DebugType>embedded</DebugType>
    <LangVersion>latest</LangVersion>
    <EmbedUntrackedSources>true</EmbedUntrackedSources>
    <GenerateAssemblyVersionAttribute>false</GenerateAssemblyVersionAttribute>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)' != 'Debug' ">
    <Optimize>true</Optimize>
  </PropertyGroup>
	
	<PropertyGroup Condition=" '$(TargetFramework)' == 'NET9_0_OR_GREATER' ">
		<DefineConstants>$(DefineConstants);NET9_0_OR_GREATER;NETCORE;NETCORE_SUPPORT</DefineConstants>
		<SignAssembly>True</SignAssembly>
		<DelaySign>False</DelaySign>
		<AssemblyOriginatorKeyFile>../servicestack.snk</AssemblyOriginatorKeyFile>
	</PropertyGroup>
</Project>
