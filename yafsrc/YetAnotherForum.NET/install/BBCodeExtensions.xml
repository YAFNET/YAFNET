<YafBBCodeList>
    <YafBBCode>
        <Name>MEDIA</Name>
        <Description>[MEDIA][localization=MEDIA]Put URL Here[/localization][/MEDIA]</Description>
        <OnClickJS />
        <DisplayJS />
        <EditJS />
        <DisplayCSS />
        <SearchRegex>\[media\](?&lt;inner&gt;.+?)\[\/media\]</SearchRegex>
        <ReplaceRegex />
